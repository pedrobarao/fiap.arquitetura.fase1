{
    "stryker-config": {
        "$schema": "https://stryker-mutator.io/stryker-config.schema.json",
        "mutate": [
            "./**/*.cs",
            "!./StrykerOutput/**"
        ],
        "test-projects": [
            {
                "path": "./Fase1.Contatos.Domain.Test/Fase1.Contatos.Domain.Test.csproj",
                "excludedMutations": [],
                "testFramework": "xunit"
            }
        ],
        "reporters": [
            "html",
            "progress"
        ],
        "thresholds": {
            "high": 80,
            "low": 60,
            "break": null
        }
    }
}